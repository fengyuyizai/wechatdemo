cover-view, cover-image{
  overflow: visible;
}

/* 地图样式 */
.mapCanvas{
  position: absolute;
  top: 83rpx;
  z-index: 1;
}

/**控件样式**/
.mapControls{
  position: absolute;
  width: 100%;
  height: 100%;
  /* z-index: 3; */
}
.mapControls .header{
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  opacity: 90;
  z-index: 3;
}
.mapControls .header .header_seach{
  border: 1px solid #ddd;
  height: 83rpx;
  font-size: 13px;
  box-sizing: border-box;
  width: 612rpx;
  display: inline-block;
  color: rgb(153, 153, 153);
  padding-left: 58rpx;
  margin-right: 13rpx;
  background-color: #fff;
  box-shadow:0px 1px 10px 0px rgba(0,0,0,0.16);
  border-radius: 4rpx;
}
.mapControls .header .seach_icon {
  position: absolute;
  align-self:center;
  width: 31rpx;
  height: 31rpx;
  left: 42rpx;
}
.mapControls .header .song_icon {
  position: absolute;
  align-self:center;
  width: 21rpx;
  height: 31rpx;
  right: 140rpx;
}
.mapControls .header .header_submit{
  height:  83rpx;
  font-size: 13px;
  text-align: center;
  width: 83rpx;
  padding: 0;
  line-height: 83rpx;
  background-color: #fff;
  color: #5A8CE7;
  display: inline-block;
  box-shadow:0px 1px 10px 0px rgba(0,0,0,0.16);
  border-radius: 4rpx;
}

.mapControls .guide{
  position: absolute;
  display: flex;
  flex-direction: column;
  margin-left: 21rpx;

  z-index: 3;
}
.mapControls .guide .compass_icon{
  width: 68rpx;
  height: 68rpx;
  margin: 25rpx 0;
}
.mapControls .guide .guide_back{
  width: 68rpx;
  height: 68rpx;
  border-radius: 50%;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #d6d6d6;
}
.mapControls .guide .guide_back .guide_icon{
  width: 35rpx;
  height: 36rpx;
}

.mapControls .rightControls {
  position: absolute;
  bottom: 42rpx;
  right: 13rpx;
  width: 74rpx;
  height: 343rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  z-index: 3;
}
.mapControls .rightControls .shijing{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 68rpx;
  height: 97rpx;
  border-radius: 34rpx;
  background-color: #fff;
  box-shadow:0px 3px 3px 0px rgba(90,140,231,0.42);
  margin: 20rpx 0;
}
.mapControls .rightControls .shijing cover-view{
  width: 46rpx;
  height: 46rpx;
  border-radius: 50%;
  background-color: #6A9AF3;
  font-size: 17rpx;
  color: #fff;
  line-height: 46rpx;
  text-align: center;
}
.mapControls .rightControls .button {
  width: 74rpx;
  height: 74rpx;
  text-align: center;
  line-height: 74rpx;
  background-color: #fff;
  color: #6A9AF3;
  font-size: 40rpx;
  border-radius: 50%;
  box-shadow:0px 3px 3px 0px rgba(90,140,231,0.42);
  margin: 15rpx 0;
}
.mapControls .rightControls .add{

}

.mapControls .leftControls{
  position: absolute;
  left: 21rpx;
  bottom: 45rpx;
  z-index: 3;
}

.mapControls .leftControls .chooseFloor{
  width: 68rpx;
  height:283rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 34rpx;
  box-shadow:0px 3px 3px 0px rgba(90,140,231,0.42);
  padding: 0 10rpx;
  padding-top: 10rpx;
}
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.mapControls .leftControls .chooseFloor .floorItem{
  width:46rpx;
  height:46rpx;
  background:rgba(223,223,223,1);
  border-radius:50%;
  font-size: 18rpx;
  line-height: 46rpx;
  text-align: center;
  margin: 7rpx 0;
}
.mapControls .leftControls .chooseFloor .floorItem cover-view{
  display: inline;
}
.mapControls .leftControls .chooseFloor .floorItem.curfloor{
  background-color: #5A8CE7;
  color: #fff;
}
.mapControls .leftControls .curFloor{
  width:68rpx;
  height:68rpx;
  border-radius:50%;
  font-size: 29rpx;
  line-height: 68rpx;
  text-align: center;
  background-color: #5A8CE7;
  color: #fff;
  margin: 21rpx 0;
}
.mapControls .leftControls .location{
  width: 42rpx;
  height: 42rpx;
  padding: 20rpx;
  border-radius: 50%;
  box-shadow:0px 3rpx 3rpx 0px rgba(90,140,231,0.42);
  background-color: #fff;
}

.comeHereModal{
	position: absolute;
	width:100%;
	height: 370rpx;
  box-sizing: border-box;
	background-color: rgba(255,255,255, 0);
  overflow: hidden;
	bottom: -370rpx;
	padding-bottom: 10rpx;
	display: flex;
	justify-content: center;
	transition: bottom 0.5s;
  z-index: 4;
}

.comeHereModal .comeHere-icon{
	position: absolute;
  z-index: 7;
  width: 178rpx;
  height: 178rpx;
	right: 69rpx;
	top: 0rpx;
}

.comeHereModal .comeHereModal_contain{
	position: relative;
  z-index: 4;
	width: 95%;
	height: 275rpx;
	background:rgba(255,255,255,1);
	border-radius:5px;
	/* box-shadow:0px 2px 12px 0px rgba(0,0,0,0.26); */
  border: 1px solid #b5b5b5;
	padding: 40rpx 59rpx 0rpx 59rpx;
  margin-top: 82rpx;
	box-sizing: border-box;
}

.comeHereModal .comeHereModal_contain .body .position{
	font-size: 19px;
	font-family:SourceHanSansCN-Regular;
	font-weight:400;
	color:#333333;
}

.comeHereModal .comeHereModal_contain .body .floor{
	font-size:16px;
	font-family:SourceHanSansCN-Regular;
	line-height: 82rpx;
	font-weight:400;
	color:rgba(90,140,231,1);
}

.comeHereModal .comeHereModal_contain .body .hr{
  height: 0.5px;
  width: 100%;
  box-sizing: border-box;
  border: 1px solid rgba(221,221,221,1);
}

.comeHereModal .comeHereModal_contain .body .shareFriend{
	height: 104rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.comeHereModal .comeHereModal_contain .body .shareFriend .share-icon{
	content: "";
	width: 31rpx;
	height: 31rpx;
	display: inline-block;
	margin-right: 13rpx;
}

.comeHereModal .comeHereModal_contain .body .shareFriend .text{
  display: inline-block;
	color: #333333;
	font-size:29rpx;
	font-family:SourceHanSansCN-Regular;
}



/* 导航信息 */
.planWayModal{
	position: absolute;
	width: 100%;
	height: 423rpx;
	background-color: #fff;
	bottom: -423rpx;
	padding-bottom: 28rpx;
  box-sizing: border-box;
	display: flex;
	justify-content: center;
	transition: bottom 0.5s;
  z-index: 6;
  overflow:hidden;
}
.planWayModal .planWayModal_contain{
	position: relative;
	width: 95%;
	height: 100%;
	background:rgba(255,255,255,1);
	border-radius:5px;
	box-shadow:0px 2px 12px 0px rgba(0,0,0,0.26);
	/* padding-top: 1.66rem; */
	/*padding: 1.66rem 2.333rem 0 2.333rem;*/
	box-sizing: border-box;
	display: flex;
	flex-direction: column;
	align-items: center;
}

.planWayModal .planWayModal_contain .title{
	font-size:15px;
	text-align: center;
	font-family:SourceHanSansCN-Regular;
	font-weight:400;
  line-height: 69rpx;
	color:rgba(153,153,153,1);
}

.planWayModal .planWayModal_contain .selectPoint{
	display: flex;
	flex-direction: row;
	justify-content: center;
  align-items: center;
	width: 86%;
	margin-bottom: 33rpx;
}

.planWayModal .planWayModal_contain .selectPoint .icon{
	display: inline-block;
	width: 28rpx;
	height:43rpx;
}

.planWayModal .planWayModal_contain .selectPoint cover-view{
	width: 90%;
	height: 76rpx;
	background:rgba(243,243,243,1);
	border-radius:5px;
	line-height: 76rpx;
	font-size: 15px;
	color: #5A8CE7;
	padding-left: 25rpx;
	display: inline-block;
	margin-left: 33rpx;
}
.planWayModal .planWayModal_contain .endPoint cover-view{
	color: #39C1E7;
}

.planWayModal .planWayModal_contain .navigationBtn{
	width: 100%;
	height:135rpx;
	display: flex;
	flex-direction: row;
	justify-content: center;
  background:rgba(246,246,246,1);
}
.planWayModal .planWayModal_contain .navigationBtn .content{
  width: 86%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.planWayModal .planWayModal_contain .navigationBtn .simulation{
	width:222rpx;
	height:76rpx;
	border:1px solid rgba(91,199,160,1);
	border-radius:18px;
	box-sizing: border-box;
	font-size:17px;
	font-family:SourceHanSansCN-Regular;
	font-weight:400;
	color:rgba(91,199,160,1);
	text-align: center;
	line-height: 76rpx;
}

.planWayModal .planWayModal_contain .navigationBtn .real{
	width:361rpx;
	height: 76rpx;
	background:rgba(90,140,231,1);
	border-radius:18px;
	font-size:17px;
	font-family:SourceHanSansCN-Regular;
	font-weight:400;
	color:rgba(255,255,255,1);
	text-align: center;
	line-height: 76rpx;
}

.quickPosition {
  position: relative;
  width: 100%;
  height: 100%;
  z-index: 6;
}
.quickPosition .wrpper{
  position: absolute;
  width: 100%;
  height: 740rpx;
  bottom: -680rpx;
  background-color: #fff;
  z-index: 8;
}
.quickPosition .wrpper .content{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.quickPosition .wrpper .content .quickPosition_item{
  width: 95%;
  height: auto;
  background-color: #f6f6f6;
  margin-bottom: 10px;
}
.quickPosition .wrpper .content .quickPosition_item .quickPosition_header{
  position: relative;
  width: 100%;
  height: 90rpx;
	padding-left: 25rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
  display: flex;
  align-items: center;
	/*border-bottom: 1px solid #666;*/
	box-shadow:0px 1px 2px 0px rgba(0,0,0,0.15);
}
.quickPosition .wrpper .content .quickPosition_item .quickPosition_header .icon{
  width: 60rpx;
  height: 60rpx;
  margin-right: 15rpx;
  float: left;
}
.quickPosition .wrpper .content .quickPosition_item .quickPosition_header .title{
  line-height: 56rpx;
	font-size: 19px;
	color: #333;
  float: left;
}
.quickPosition .wrpper .content .quickPosition_item .quickPosition_header .title .describe{
  display: inline;
  font-size: 12px;
	color: #666;
}
.quickPosition .wrpper .content .quickPosition_item .quickPosition_header .down{
  position: absolute;
  right: 40rpx;
	width: 30rpx;
  height: 20rpx;
  margin-top: 0px;
  transition: all 0.5s;
}
.quickPosition .wrpper .content .quickPosition_item .body{
  padding: 30rpx 25rpx;
	box-sizing: border-box;
  display: flex;
  width: 100%;
  flex-direction: row;
  flex-wrap:wrap;
}
.quickPosition .wrpper .content .quickPosition_item .body .body_item{
  line-height: 50rpx;
	display: inline-block;
	height: 50rpx;
	margin: 0 25rpx 25rpx 0;
	/* background-color: #11daa4; */
	border-radius: 30rpx;
	padding: 10rpx 30rpx;
	color: #fff;
  font-size: 16px;
	box-shadow:0px 1px 2px 0px rgba(0,0,0,0.15);
}

.quickPosition .bgmTransparent{
  width: 100%;
  height: 100%;
  background-color: rgba(56,56,56,0.3);
}